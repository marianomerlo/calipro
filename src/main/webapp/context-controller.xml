<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
					http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="productController" class="ar.edu.utn.frba.proyecto.controller.ProductController" scope="session">
		<property name="ITEM_NAME" value="Producto" />
		<property name="productDao" ref="productDao" />
	</bean>

	<bean id="analisisController" class="ar.edu.utn.frba.proyecto.controller.AnalisisController" scope="session">
		<property name="ITEM_NAME" value="Analisis" />
		<property name="analisisDao" ref="analisisDao" />
	</bean>

	<bean id="userController" class="ar.edu.utn.frba.proyecto.controller.UserController" scope="session">
		<property name="ITEM_NAME" value="Usuario" />
		<property name="userDao" ref="userDao" />
	</bean>

</beans>
