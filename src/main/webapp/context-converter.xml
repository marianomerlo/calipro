<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
					http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="productConverter" class="ar.edu.utn.frba.proyecto.converter.ProductConverter"
		scope="session">
		<property name="productController" ref="productController" />
	</bean>

	<bean id="analisisConverter" class="ar.edu.utn.frba.proyecto.converter.AnalisisConverter"
		scope="session">
		<property name="analisisController" ref="analisisController" />
	</bean>

	<bean id="bandaConverter" class="ar.edu.utn.frba.proyecto.converter.BandaConverter"
		scope="session">
		<property name="bandaController" ref="bandaController" />
	</bean>

	<bean id="sectorConverter" class="ar.edu.utn.frba.proyecto.converter.SectorConverter"
		scope="session">
		<property name="estadioController" ref="estadioController" />
	</bean>
	<bean id="filaConverter" class="ar.edu.utn.frba.proyecto.converter.FilaConverter"
		scope="session">
		<property name="estadioController" ref="estadioController" />
	</bean>
	<bean id="asientoConverter" class="ar.edu.utn.frba.proyecto.converter.AsientoConverter"
		scope="session">
		<property name="estadioController" ref="estadioController" />
	</bean>
	
	<bean id="festivalConverter" class="ar.edu.utn.frba.proyecto.converter.FestivalConverter"
		scope="session">
		<property name="festivalController" ref="festivalController" />
	</bean>

	<bean id="diaConverter" class="ar.edu.utn.frba.proyecto.converter.DiaConverter"
		scope="session">
		<property name="estadioController" ref="estadioController" />
	</bean>
</beans>
